{
  "context": {
    "fileName": [
      "AGENT.md",
      "GEMINI.md",
      "CLAUDE.md",
      ",CLAUDE.md",
      ",GEMINI.md"
    ]
  },
  "mcpServers": {
    "memory": {
      "trust": true,
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "sequentialthinking": {
      "trust": true,
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "serena": {
      "trust": true,
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ],
      "command": "uvx"
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "GitHub",
    "dynamicWindowTitle": false,
    "showStatusInTitle": true,
    "showMemoryUsage": true,
    "showCitations": true
  },
  "general": {
    "previewFeatures": true,
    "enablePromptCompletion": true
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "experimental": {
    "skills": true
  }
}
